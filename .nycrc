{
  "all": true,
  "include": [
    "api/**/*.js",
    "utils/**/*.js",
    "scripts/**/*.js"
  ],
  "exclude": [
    "tests/**/*.js",
    "coverage/**/*.js"
  ],
  "reporter": [
    "text",
    "lcov"
  ],
  "check-coverage": true,
  "branches": 70,
  "lines": 80,
  "functions": 80,
  "statements": 80
}